{
  "name": "coretext",
  "version": "0.1.0",
  "description": "CoreText: Local Knowledge Graph & MCP Server",
  "mcpServers": {
    "coretext": {
      "command": "poetry",
      "args": [
        "run",
        "coretext",
        "adapter"
      ],
      "cwd": "${extensionPath}",
      "env": {
        "PYTHONPATH": "${extensionPath}"
      }
    }
  }
}